{
    "name": "brunocfalcao/eduka-installer",
    "description": "Eduka installer",
    "keywords": [
        "brunocfalcao"
    ],
    "homepage": "https://github.com/brunocfalcao/eduka-installer",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "bruno.c.falcao@brunofalcao.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "brunocfalcao/eduka-nereus": "dev-master",
        "brunocfalcao/eduka-abstracts": "dev-master",
        "illuminate/contracts": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Eduka\\Installer\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Eduka\\Installer\\InstallerServiceProvider"
            ]
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}
